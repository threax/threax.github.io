{
  "$schema": "https://www.threax.com/EdityMcEditface/edityschema.json",
  "vars": {
    "siteTitle": "Threax Software"
  },
  "contentMap": {

  },
  "compilers": [
    {
      "type": "Html",
      "extension": ".html"
    }
  ],
  "preBuildTasks": [
    {
      "name": "GetPublishRepo",
      "settings": {
        "url": "https://github.com/threax/threax.github.io.git"
      }
    }
  ],
  "postBuildTasks": [
    {
      "name": "AddGithubCname",
      "settings": {
        "host": "www.threax.com"
      }
    }
  ],
  "postPublishTasks": [
    {
      "name" : "PublishToGitRepo"
    }
  ]
}